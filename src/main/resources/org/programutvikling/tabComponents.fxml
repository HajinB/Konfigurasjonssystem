                            <Insets top="5.0" />
                        </GridPane.margin>
                    </Button>
                    <Label fx:id="lblComponentMsg" prefHeight="65.0" prefWidth="230.0" textFill="#388dc8" GridPane.rowIndex="9">
                        <font>
                            <Font size="13.0" />
                        </font>
                    </Label>
                    <ChoiceBox id="productType" fx:id="cbType" prefHeight="30.0" prefWidth="230.0" styleClass="cb" stylesheets="@CSS/Superbruker.css" GridPane.rowIndex="1">
                        <GridPane.margin>
                            <Insets />
                        </GridPane.margin>
                    </ChoiceBox>
               <columnConstraints>
                  <ColumnConstraints />
               </columnConstraints>
                </GridPane>
            </Pane>
            <Label layoutX="361.0" layoutY="61.0" prefHeight="28.0" prefWidth="283.0" text="Komponenter i nettbutikk">
                <font>
                    <Font name="Verdana Bold" size="17.0" />
                </font>
            </Label>
            <TableView fx:id="tblViewComponent" editable="true" layoutX="358.0" layoutY="98.0" onMousePressed="#dblClickTblRow" prefHeight="478.0" prefWidth="645.0" styleClass="tblView" stylesheets="@CSS/Superbruker.css">
                <columns>
                    <TableColumn minWidth="100.0" prefWidth="120.0" styleClass="column" text="Produkttype">
                        <cellValueFactory>
                            <PropertyValueFactory property="productType" />
                        </cellValueFactory>
                    </TableColumn>
                    <TableColumn minWidth="100.0" prefWidth="150.0" styleClass="column" text="Produktnavn">
                        <cellValueFactory>
                            <PropertyValueFactory property="productName" />
                        </cellValueFactory>
                    </TableColumn>
                    <TableColumn minWidth="100" prefWidth="230.0" styleClass="column" text="Beskrivelse">
                        <cellValueFactory>
                            <PropertyValueFactory property="productDescription" />
                        </cellValueFactory>
                    </TableColumn>
                    <TableColumn fx:id="productPriceColumn" minWidth="100" prefWidth="140.0" text="Pris">
                        <cellValueFactory>
                            <PropertyValueFactory property="productPrice" />
                        </cellValueFactory>
                  <styleClass>
                     <String fx:value="columnPrice" />
                     <String fx:value="column" />
                  </styleClass>
                    </TableColumn>
                </columns>
                <contextMenu>
                    <ContextMenu fx:id="cm">
                        <items>
                            <MenuItem mnemonicParsing="false" onAction="#cmDeleteRow" text="Delete row" />
                        </items>
                    </ContextMenu>
                </contextMenu>
            </TableView>
            <Label layoutX="426.0" layoutY="8.0" text="Velkommen *navn* ">
                <font>
                    <Font name="Verdana Bold" size="26.0" />
                </font>
            </Label>
            <Separator layoutX="28.0" layoutY="50.0" prefHeight="6.0" prefWidth="954.0" />
            <Separator layoutX="339.0" layoutY="134.0" orientation="VERTICAL" prefHeight="518.0" prefWidth="8.0" />
            <TextField fx:id="componentSearch" layoutX="361.0" layoutY="590.0" onKeyTyped="#search" prefHeight="30.0" prefWidth="170.0" promptText="Skriv inn søkeord" styleClass="input" stylesheets="@CSS/Superbruker.css">
                <font>
                    <Font size="13.0" />
                </font>
            </TextField>
            <ChoiceBox fx:id="cbTypeFilter" layoutX="542.0" layoutY="590.0" onAction="#filterByTypeSelected" prefHeight="30.0" prefWidth="170.0" styleClass="cb" stylesheets="@CSS/Superbruker.css" />
            <Button layoutX="871.0" layoutY="590.0" mnemonicParsing="false" onAction="#btnDelete" prefHeight="30.0" prefWidth="131.0" styleClass="btnDelete" stylesheets="@CSS/Superbruker.css" text="Slett produkt" />
            <ComboBox fx:id="cbRecentFiles" layoutX="732.0" layoutY="60.0" prefHeight="30.0" prefWidth="146.0" promptText="Åpne nylige filer" styleClass="cb" stylesheets="@CSS/Superbruker.css" />
            <Button layoutX="889.0" layoutY="60.0" mnemonicParsing="false" onAction="#btnOpenRecentFile" prefHeight="30.0" prefWidth="110.0" stylesheets="@CSS/Login.css" text="Åpne" />
            <Label layoutX="29.0" layoutY="62.0" prefHeight="26.0" prefWidth="222.0" text="Legg til ny komponent">
                <font>
                    <Font name="Verdana Bold" size="17.0" />
                </font>
            </Label>
        </AnchorPane>
    </ScrollPane>

</AnchorPane>
