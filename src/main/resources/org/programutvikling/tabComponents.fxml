<AnchorPane fx:id="topLevelPane" prefHeight="700.0" prefWidth="1040.0" stylesheets="@CSS/Superbruker.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.programutvikling.gui.TabComponentsController">
    <ScrollPane pannable="true" prefHeight="700.0" prefWidth="1040.0" stylesheets="@CSS/Superbruker.css">
        <AnchorPane prefHeight="700.0" prefWidth="1040.0" styleClass="anchorCenter" stylesheets="@CSS/Superbruker.css">
            <Pane layoutX="29.0" layoutY="98.0" prefHeight="524.0" prefWidth="296.0" styleClass="nyKomponentPane" stylesheets="@CSS/Superbruker.css">
                <GridPane fx:id="componentRegNode" layoutX="33.0" layoutY="29.0" prefHeight="477.0" prefWidth="230.0" vgap="4.0">
                     <rowConstraints>
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <padding>
                        <Insets bottom="2.0" />
                    </padding>
                    <Label prefHeight="16.0" prefWidth="135.0" text="Produkttype: ">
                        <font>
                            <Font name="Verdana" size="13.0" />
                        </font>
                    </Label>
                    <Label prefHeight="16.0" prefWidth="154.0" text="Produktnavn: " GridPane.rowIndex="2">
                        <font>
                            <Font name="Verdana" size="13.0" />
                        </font>
                        <GridPane.margin>
                            <Insets top="5.0" />
                        </GridPane.margin>
                    </Label>
                    <TextField id="productName" prefHeight="30.0" prefWidth="200.0" promptText="Skriv inn varenavn" styleClass="input" stylesheets="@CSS/Superbruker.css" GridPane.rowIndex="3">
                        <font>
                            <Font size="13.0" />
                        </font>
                    </TextField>
                    <Label prefHeight="16.0" prefWidth="151.0" text="Beskrivelse: " GridPane.rowIndex="4">
                        <font>
                            <Font name="Verdana" size="13.0" />
                        </font>
                        <GridPane.margin>
                            <Insets top="5.0" />
                        </GridPane.margin>
                    </Label>
                    <TextArea id="productDescription" prefHeight="150.0" prefWidth="200.0" promptText="Gi en kort beskrivelse av varen. For eksempel størrelse, effekt etc.. " styleClass="input" stylesheets="@CSS/Superbruker.css" GridPane.rowIndex="5">
                        <font>
                            <Font name="Verdana" size="13.0" />
                        </font>
                    </TextArea>
                    <Label prefHeight="16.0" prefWidth="56.0" text="Pris:" GridPane.rowIndex="6">
                        <font>
                            <Font size="13.0" />
                        </font>
                        <GridPane.margin>
                            <Insets top="5.0" />
                        </GridPane.margin>
                    </Label>
                    <PriceField id="productPrice" prefHeight="30.0" prefWidth="200.0" promptText="Skriv inn pris" styleClass="input" stylesheets="@CSS/Superbruker.css" GridPane.rowIndex="7">
                        <font>
                            <Font size="13.0" />
                        </font>
                    </PriceField>
                    <Button mnemonicParsing="false" onAction="#btnAddComponent" prefHeight="30.0" prefWidth="230.0" stylesheets="@CSS/Login.css" text="Legg til komponent" GridPane.rowIndex="8">
