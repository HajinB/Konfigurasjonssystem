                    <Font name="Verdana" size="13.0" />
                </font>
                <GridPane.margin>
                    <Insets top="5.0" />
                </GridPane.margin>
            </Label>
            <TextField id="popupproductName" fx:id="txtPopupProductName" prefHeight="30.0" prefWidth="200.0" styleClass="input" stylesheets="@CSS/Superbruker.css" GridPane.rowIndex="3">
                <font>
                    <Font size="13.0" />
                </font>
            </TextField>
            <Label prefHeight="16.0" prefWidth="98.0" text="Beskrivelse" GridPane.rowIndex="4">
