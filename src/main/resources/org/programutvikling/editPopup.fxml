                    <Insets top="5.0" />
                </GridPane.margin>
            </Label>
            <PriceField id="popupproductPrice" fx:id="txtPopupProductPrice" prefHeight="30.0" prefWidth="200.0" promptText="Skriv inn pris" styleClass="input" stylesheets="@CSS/Superbruker.css" GridPane.rowIndex="7">
