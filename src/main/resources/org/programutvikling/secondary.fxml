<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane fx:id="topLevelPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="775.0" prefWidth="1040.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.programutvikling.gui.MainController">
   <top>
      <AnchorPane prefHeight="30.0" prefWidth="1040.0" BorderPane.alignment="CENTER">
         <children>
            <MenuBar layoutY="-3.0" prefHeight="30.0" prefWidth="1040.0" styleClass="menyBar" stylesheets="@CSS/Superbruker.css">
              <menus>
                <Menu mnemonicParsing="false" text="Fil">
                  <items>
                        <MenuItem mnemonicParsing="false" onAction="#btnSetDirectory" text="Velg lagringsplass" />
                        <MenuItem mnemonicParsing="false" onAction="#btnSaveToChosenPath" text="Lagre som..." />
                        <MenuItem mnemonicParsing="false" onAction="#btnOpenJobj" text="Åpne jobj-fil" />
                        <MenuItem mnemonicParsing="false" onAction="#btnLogOut" text="Logg ut" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Verktøy">
                  <items>
                    <MenuItem mnemonicParsing="false" onAction="#btnRemoveDuplicates"
                              text="Fjern duplikater fra registeret" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
         </children>
      </AnchorPane>
   </top>
   <center>
      <TabPane prefHeight="750.0" prefWidth="1040.0" BorderPane.alignment="CENTER">
         <tabs>
            <Tab fx:id="tabComponentRegister" text="Komponenter">
               <content>
                  <fx:include fx:id="tabComponents" source="tabComponents.fxml" />
               </content>
            </Tab>
            <Tab fx:id="tabUserRegister" text="Brukere">
               <content>
                  <fx:include fx:id="tabUsers" source="tabUsers.fxml" />
               </content>
            </Tab>
         </tabs>
      </TabPane>
   </center>
</BorderPane>
